{
  "name": "@pendle/constants",
  "author": "Pendle Labs",
  "version": "1.0.0-beta-5",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
