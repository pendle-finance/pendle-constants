    "singleQuote": true,
    "trailingComma": "es5"
  },
  "module": "dist/constants.esm.js",
  "devDependencies": {
    "husky": "^8.0.3",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "ethers": "^5.7.2"
  }
